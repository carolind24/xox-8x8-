int main()
{
    int s, sira=1;
 	printf("(Oyunu bitirmek icin 'Q' girin!)\n");
 	printf("(Bos veya yanlis hamle girmeniz sirayi rakibinize gecirir!)\n");
 	printf("(Hamleleri tek girdide ornegin 'A3' seklinde giriniz!)ç\n");
 	printf("(Rakibin bir tasini yemediginiz bir hamleyi yapamazsiniz!)\n\n");
    
	while(1)
    {
		oyun(sira);
        s = hamle(sira);
        sira++;
    }
 
    return 0;
}

char sira(int s)			//HER HAMLEDE SİR++ OLDUKCA SECİM FONKSİYONUNDAN GELEN DEGER X VEYA O DONDURUR.
{							//SİR=1 OLDUGU İCİN OYUNA O BASLAR.
    if((s%2) == 0)
        return 'X';
    else
        return 'O'; 
}

 
void oyun(int si)
{
    int i=0, j=0;
 
 									
    if((si % 2) == 0)
        printf("\tSira : X\n");
    else
        printf("\tSira : O\n");
 
    printf("  1 2 3 4 5 6 7 8 \n");
    for(i=0; i < 8; i++)
    {											//BURADA DİZİYİ OLUSTUYORUZ.
        for(j=0; j < 9; j++)
        {
            printf("%c", a[i][j]);
            printf(" ");
        }
        printf("\n");
    } 
}
 
int hamle(int sir)
{
