							k==5	&& a[6][5] == '.' && a[6][3] == ss && a[6][4] != ss && a[6][4] != '.' ||
							k==5	&& a[6][5] == '.' && a[6][7] == ss && a[6][6] != ss && a[6][6] != '.' )
						a[6][5] = ss;
				
				
				else if(	k==6	&& a[6][6] == '.' && a[4][4] == ss && a[5][5] != ss && a[5][5] != '.' ||
							k==6	&& a[6][6] == '.' && a[4][8] == ss && a[5][7] != ss && a[5][7] != '.' ||
							k==6	&& a[6][6] == '.' && a[4][6] == ss && a[5][6] != ss && a[5][6] != '.' ||
							k==6	&& a[6][6] == '.' && a[6][4] == ss && a[6][5] != ss && a[6][5] != '.' ||
							k==6	&& a[6][6] == '.' && a[6][8] == ss && a[6][7] != ss && a[6][7] != '.' )
						a[6][6] = ss;
				
				
				else if(	k==7	&& a[6][7] == '.' && a[4][5] == ss && a[5][6] != ss && a[5][6] != '.' ||
							k==7	&& a[6][7] == '.' && a[4][7] == ss && a[5][7] != ss && a[5][7] != '.' ||
							k==7	&& a[6][7] == '.' && a[6][5] == ss && a[6][6] != ss && a[6][6] != '.' )
						a[6][7] = ss;
			
			
				else if(	k==8	&& a[6][8] == '.' && a[4][6] == ss && a[5][7] != ss && a[5][7] != '.' ||
							k==8	&& a[6][8] == '.' && a[4][8] == ss && a[5][8] != ss && a[5][8] != '.' ||
							k==8	&& a[6][8] == '.' && a[6][6] == ss && a[6][7] != ss && a[6][7] != '.' )																				
						a[6][8] = ss;

				else{
					printf("\nBuraya hamle yapamazsiniz!\n");
				}
				break;
				
			case 'H':
			case 'h':
				scanf("%d", &k);
				kont=1;
								if(k==1	&& a[7][1] == '.' && a[5][3] == ss && a[6][2] != ss && a[6][2] != '.' ||
				   k==1	&& a[7][1] == '.' && a[5][1] == ss && a[6][1] != ss && a[6][1] != '.' ||
				   k==1	&& a[7][1] == '.' && a[7][3] == ss && a[7][2] != ss && a[7][2] != '.')
				       
					a[7][1] = ss;
				
				
				else if(	k==2	&& a[7][2] == '.' && a[5][4] == ss && a[6][3] != ss && a[6][3] != '.' ||
						k==2	&& a[7][2] == '.' && a[5][2] == ss && a[6][2] != ss && a[6][2] != '.' ||
						k==2	&& a[7][2] == '.' && a[7][4] == ss && a[7][3] != ss && a[7][3] != '.' )
						
					a[7][2] = ss;
				
				
				else if(	k==3	&& a[7][3] == '.' && a[5][1] == ss && a[6][2] != ss && a[6][2] != '.' ||
						k==3	&& a[7][3] == '.' && a[5][5] == ss && a[6][4] != ss && a[6][4] != '.' ||
						k==3	&& a[7][3] == '.' && a[5][3] == ss && a[6][3] != ss && a[6][3] != '.' ||
						k==3	&& a[7][3] == '.' && a[7][1] == ss && a[7][2] != ss && a[7][2] != '.' ||
						k==3	&& a[7][3] == '.' && a[7][5] == ss && a[7][4] != ss && a[7][4] != '.' )
					a[7][3] = ss;
				
				
				else if(	k==4	&& a[7][4] == '.' && a[5][2] == ss && a[6][3] != ss && a[6][3] != '.' ||
						k==4	&& a[7][4] == '.' && a[5][6] == ss && a[6][5] != ss && a[6][5] != '.' ||
						k==4	&& a[7][4] == '.' && a[5][4] == ss && a[6][4] != ss && a[6][4] != '.' ||
						k==4	&& a[7][4] == '.' && a[7][2] == ss && a[7][3] != ss && a[7][3] != '.' ||
						k==4	&& a[7][4] == '.' && a[7][6] == ss && a[7][5] != ss && a[7][5] != '.' )
					a[7][4] = ss;
				
				
				else if(	k==5	&& a[7][5] == '.' && a[5][3] == ss && a[6][4] != ss && a[6][4] != '.' ||
						k==5	&& a[7][5] == '.' && a[5][7] == ss && a[6][6] != ss && a[6][6] != '.' ||
						k==5	&& a[7][5] == '.' && a[5][5] == ss && a[6][5] != ss && a[6][5] != '.' ||
						k==5	&& a[7][5] == '.' && a[7][3] == ss && a[7][4] != ss && a[7][4] != '.' ||
						k==5	&& a[7][5] == '.' && a[7][7] == ss && a[7][6] != ss && a[7][6] != '.' )
					a[7][5] = ss;
				
				
				else if(		k==6	&& a[7][6] == '.' && a[5][4] == ss && a[6][5] != ss && a[6][5] != '.' ||
							k==6	&& a[7][6] == '.' && a[5][8] == ss && a[6][7] != ss && a[6][7] != '.' ||
							k==6	&& a[7][6] == '.' && a[5][6] == ss && a[6][6] != ss && a[6][6] != '.' ||
							k==6	&& a[7][6] == '.' && a[7][4] == ss && a[7][5] != ss && a[7][5] != '.' ||
							k==6	&& a[7][6] == '.' && a[7][8] == ss && a[7][7] != ss && a[7][7] != '.' )
								a[7][6] = ss;
				
				
				else if(	k==7	&& a[7][7] == '.' && a[5][5] == ss && a[6][6] != ss && a[6][6] != '.' ||
						k==7	&& a[7][7] == '.' && a[5][7] == ss && a[6][7] != ss && a[6][7] != '.' ||
						k==7	&& a[7][7] == '.' && a[7][5] == ss && a[7][6] != ss && a[7][6] != '.' )
					a[7][7] = ss;
			
			
				else if(	k==8	&& a[7][8] == '.' && a[5][6] == ss && a[6][7] != ss && a[6][7] != '.' ||
						k==8	&& a[7][8] == '.' && a[5][8] == ss && a[6][8] != ss && a[6][8] != '.' ||
						k==8	&& a[7][8] == '.' && a[7][6] == ss && a[7][7] != ss && a[7][7] != '.' )																				
					a[7][8] = ss;


				else{
					printf("\nBuraya hamle yapamazsiniz!\n");
				}	
					break;
				
				/*default:
						DEFAULT CASE'İNE YAZILAN HERSEYİ CASE'E GİRSEDE GİRMESEDE		
						KULLANIYOR,YAPILAMAYACAK HAMLELER SONUCU DURUMLAR BURAYA YAZILMALI.(YAPILAN HER HAMLEDE DEFAULTA GİRİYOR)  
						BU YUZDEN KULLANAMADIM.	*/											
		 }
            	 
		}
				
	return kontrol(ss, sir ,2);
	
 
}
 
int kontrol(char sss, int sir, int kazanan)
{	
	int i,j;
	int sayx = 0;	
	int sayo = 0;
		
		for(i=0;i<9;i++){
    		for(j=1;j<7;j++){
    			if(a[i][j]=='X' && a[i][j+2]=='X' && a[i][j+1] != '.')			//ÜST-ALT , SAG-SOL  VE SAG-SOL CAPRAZLARDA DEGERLER AYNIYSA
    				{
					a[i][j+1] = 'X';											//VE ORTA '.''DAN FARKLIYSA ORTANCA DEGERİ X/O DEĞİŞTİRİYORUZ.				
					sayx+=1;}
																		//HER BİRİ İÇİN BİR ADET FOR DONGUSU (ÜST-ALT-SAG-SOL-SAGCAPRAZ-SOLCAPRAZ)
				}
			}
		
		for(i=0;i<9;i++){
    		for(j=1;j<7;j++){
    			if(a[i][j]=='O' && a[i][j+2]=='O' && a[i][j+1] != '.')
    				{
					a[i][j+1] = 'O';
					sayo+=1;															//SAYX X'İN YEDİĞİ O SAYISINI,SAYO O'NUN YEDİGİ X SAYISINI SAYIYOR.	
					}																	//SIRA FONKSİYONUNDA Q GİRİLENCE KAZANAN PARAMETRESİ 1 ALIR VE ASAGIDAKİ İF BLOGU
				}																		//NA GİRMEK ZORUNDA KALIR.BOYLECE KAZANAN BELİRLENİR.
			}
		
		for(j=1;j<9;j++){
			for(i=0;i<8;i++){
				if(a[i][j]=='X' && a[i+2][j]=='X' && a[i+1][j] != '.')
					{
					a[i+1][j] = 'X';
					sayx+=1;
					}
				
			}
		}
		
		for(j=1;j<9;j++){
			for(i=0;i<8;i++){
				if(a[i][j]=='O' && a[i+2][j]=='O' && a[i+1][j] != '.')
					{
					a[i+1][j] = 'O';
					sayo+=1;
					}
				
			}
		}
		
		for(j=1;j<9;j++){
			for(i=0;i<8;i++){
				if(a[i][j]=='X' && a[i+2][j+2]=='X' && a[i+1][j+1] != '.')
					{
					a[i+1][j+1] = 'X';
					sayx+=1;
					}
				
			}
		}
		
		for(j=1;j<9;j++){
			for(i=0;i<8;i++){
				if(a[i][j]=='O' && a[i+2][j+2]=='O' && a[i+1][j+1] != '.')
					{
					a[i+1][j+1] = 'O';
					sayo+=1;
					}
				
			}
		}
		
		for(j=1;j<9;j++){
			for(i=0;i<8;i++){
				if(a[i+2][j]=='X' && a[i][j+2]=='X' && a[i+1][j+1] != '.')
					{
					a[i+1][j+1] = 'X';
					sayx+=1;
					}
				
			}
		}
		
		for(j=1;j<9;j++){
			for(i=0;i<8;i++){
				if(a[i+2][j]=='O' && a[i][j+2]=='O' && a[i+1][j+1] != '.')
					{
					a[i+1][j+1] = 'O';
					sayo+=1;
					}
				
			}
		}
	if(kazanan<2)    							
    {
        if(sayx>sayo){												//KAZANAN VE KAYBEDEN BURADA BELİRLENİR.
        	printf("Oyun bitti!\n");
			printf("\n\n\t\tKAZANAN X\n");
			getch();
			exit(0);
        return 10;
		}
		else if(sayx==sayo){
			printf("Oyun bitti!\n");
			printf("\n\n\t\tBERABERE\n");
			getch();
			exit(0);
		}
		else{
			printf("Oyun bitti!\n");
			printf("\n\n\t\tKAZANAN O\n");
			getch();
			exit(0);
		}
    }
    return 1;
}
 

